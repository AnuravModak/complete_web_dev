  <div class="row tasks col-12 col-sm-12  ">
    <div id="accordion   ">
        <div class="card">
          <div class="card-header"  id="headingOne">
            <h5 class="mb-0">
              <button class="btn " data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                Add Patients
              </button>
            </h5>
          </div>
      
          <div id="collapseOne" class="collapse show" aria-labelledby="headingOne" data-parent="#accordion">
            <div class="card-body">
                
                <form id ="add">
                    <div class="form-row">
                        <div class="form-group col-sm-5">
                                
                            <input type="text" class="form-control form-control-sm mr-1" id="firstname" name="firstname"placeholder="Firstname">
                        </div>
                        <div class="form-group col-sm-5">
                            
                            <input type="text" class="form-control form-control-sm mr-1"  id="lastname" name="lastname"placeholder="Lastname">
                        </div>
                        <div class="form-group col-sm-5">
                            
                            <input type="text"class="form-control form-control-sm mr-1" id="covid" name="covid"  placeholder="Enter covid Status [Positive: Enter '0', Negative: Enter '1']">
                        </div>
                        <div class="form-group col-sm-5">
                          
                          <input type="text" class="form-control form-control-sm mr-1" id="bed" name="bed"  placeholder="Allocate Bed">
                      </div>
                      
                        
                    <div class=" row form-row col-12 col-sm-12">
                        <button type="button" class="btn btn-center btn-sm ml-auto " style="color: black;" data-dismiss="modal">Cancel</button>
                        <button type="submit" class="btn btn-center">Add Patient</button>    
                    </div>
                </form>
            </div>
          </div>
        </div>
        <div class="card">
          <div class="card-header"  id="headingTwo">
            <h5 class="mb-0">
              <button class="btn  collapsed" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
                Remove Patients
              </button>
            </h5>
          </div>
          <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion">
            <div class="card-body">

                <form id ="remove">
                    <div class="form-row">
                        <div class="form-group col-sm-5">
                                
                            <input type="text" class="form-control form-control-sm mr-1" id="firstname" name="firstname"placeholder="Firstname">
                        </div>
                        <div class="form-group col-sm-5">
                            
                            <input type="text" class="form-control form-control-sm mr-1"  id="lastname" name="lastname"placeholder="Lastname">
                        </div>
                        
                        <div class="form-group col-sm-5">
                          
                          <input type="text" class="form-control form-control-sm mr-1" id="bed" name="bed"  placeholder="Enter Bed Id">
                      </div>
                      
                    </div>
                    <div class="form-row">
                        <button type="button" class="btn btn-center btn-sm ml-auto " style="color: black;" data-dismiss="modal">Cancel</button>
                        <button type="submit" class="btn btn-center">Remove Patient</button>    
                    </div>
                </form>
            </div>
          </div>
        </div>
        
        <div class="card"style="color: black;">
          <div class="card-header"  id="headingThree">
            <h5 class="mb-0">
              <button class="btn " data-toggle="collapse" data-target="#collapseThree" aria-expanded="false" aria-controls="collapseThree">
               Update Patients
              </button>
            </h5>
          </div>
          <div id="collapseThree" class="collapse" aria-labelledby="headingThree" data-parent="#accordion">
            <div class="card-body">

                <form id ="update">
                    <div class="form-row">
                        <div class="form-group col-sm-5">
                                
                            <input type="text" class="form-control form-control-sm mr-1" id="firstname" name="firstname"placeholder="Firstname">
                        </div>
                        <div class="form-group col-sm-5">
                            
                            <input type="text" class="form-control form-control-sm mr-1"  id="lastname" name="lastname"placeholder="Lastname">
                        </div>
                        <div class="form-group col-sm-5">
                            
                            <input type="text"class="form-control form-control-sm mr-1" id="covid" name="covid"  placeholder="Update Covid Status  ">
                        </div>
                        <div class="form-group col-sm-5">
                            
                          <input type="text"class="form-control form-control-sm mr-1" id="bed" name="bed"  placeholder="Enter Bed Number  ">
                      </div>
                        
                        
                    </div>
                    <div class="form-row">
                        <button type="button" class="btn btn-center btn-sm ml-auto " style="color: black;" data-dismiss="modal">Cancel</button>
                        <button type="submit" class="btn btn-center">Update</button>  
                         
                    </div>
                </form>

            </div>
          </div>
        </div>
